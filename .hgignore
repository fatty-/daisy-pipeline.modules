syntax: glob

# Backup files left behind by the Emacs editor.
*~

# Lock files used by the Emacs editor.
# Notice that the "#" character is quoted with a backslash.
# This prevents it from being interpreted as starting a comment.
.\#*

# Temporary files used by the vim editor.
.*.swp

# A hidden file created by the Mac OS X Finder.
.DS_Store

# switch to regexp syntax.
syntax: regexp

# ignore all output/ directories
(^|/)output/

# ignore all bin/ directories
(^|/)bin/

# ignore all generated/ directories
(^|/)generated/
# ignore all temp/ directories
(^|/)temp/

# ignore BndTools runtime directories
(^|/)runtimefw/

# ignore Maven target directories
(^|/)target/
# ignore metadata dirs 
(^|/).metadata/


